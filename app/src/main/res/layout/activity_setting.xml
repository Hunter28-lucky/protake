<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/activity_setting_0"
    android:background="#000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/root_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0">
        <View
            android:id="@+id/view_bottom"
            android:background="#161616"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
        <View
            android:background="#161616"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/root"
            android:background="#161616"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <View
                android:id="@+id/view_top"
                android:background="@color/colorBlack"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/ll_top"
                android:background="#000000"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"/>
                <TextView
                    android:textColor="#ffffff"
                    android:gravity="center"
                    android:id="@+id/tv_sign_in"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/SETTINGS_TAB_PRO"
                    android:textAllCaps="true"/>
                <TextView
                    android:textColor="#000000"
                    android:gravity="center"
                    android:id="@+id/tv_record"
                    android:background="#ffffff"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/SETTINGS_TAB_RECORD"
                    android:textAllCaps="true"/>
                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <TextView
                        android:textColor="#ffffff"
                        android:gravity="center"
                        android:id="@+id/tv_data"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/SETTINGS_TAB_DATA"
                        android:textAllCaps="true"/>
                    <ImageView
                        android:layout_gravity="end|center_vertical"
                        android:id="@+id/iv_data_lock"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_20_lock"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <TextView
                        android:textColor="#ffffff"
                        android:gravity="center"
                        android:id="@+id/tv_preset"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/SETTINGS_TAB_PRESETS"
                        android:textAllCaps="true"/>
                    <ImageView
                        android:layout_gravity="end|center_vertical"
                        android:id="@+id/iv_preset_lock"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_20_lock"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <TextView
                        android:textColor="#ffffff"
                        android:gravity="center"
                        android:id="@+id/tv_accessories"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/SETTINGS_TAB_ACCESSORIES"
                        android:textAllCaps="true"/>
                    <ImageView
                        android:layout_gravity="end|center_vertical"
                        android:id="@+id/iv_access_lock"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_20_lock"/>
                </FrameLayout>
                <TextView
                    android:textColor="#ffffff"
                    android:gravity="center"
                    android:id="@+id/tv_pro"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="@string/SETTINGS_TAB_PRO"
                    android:textAllCaps="true"/>
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/close_iv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/icon_30_close"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"/>
                    <View
                        android:id="@+id/close"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"/>
                </RelativeLayout>
            </LinearLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <com.blink.academy.film.widgets.setting.RecordContainerView
                    android:id="@+id/record_view"
                    android:background="#00ffffff"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.DataSettingView
                    android:id="@+id/data_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.PresetSettingView
                    android:id="@+id/preset_view"
                    android:background="#00ffffff"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.accessories.AccessoriesContainerView
                    android:id="@+id/accessories_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.ProtakeSignInSettingView
                    android:id="@+id/protake_sign_in_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.ProtakeProSettingView
                    android:id="@+id/protake_pro_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </FrameLayout>
        </LinearLayout>
        <com.blink.academy.film.widgets.SubscribeDialog
            android:id="@+id/subscribe_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingCodeDialog
            android:id="@+id/code_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingGimbalDialog
            android:id="@+id/gimbal_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingProtakeFocusDialog
            android:id="@+id/protake_focus_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingHintDialog
            android:id="@+id/hint_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <View
            android:id="@+id/click_view"
            android:background="@color/colorDiaBg"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:id="@+id/privacy_dialog"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <com.blink.academy.film.widgets.dialog.login.PrivacyWebView
                android:id="@+id/privacy_web_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
