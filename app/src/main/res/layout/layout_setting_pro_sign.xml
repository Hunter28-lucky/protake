<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_setting_pro_sign_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <ImageView
            android:id="@+id/iv_protake_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_100_protake_plus"
            android:tint="@color/colorWhite"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/rel_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iv_protake_logo">
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/tv_phone_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/tv_phone_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/tv_phone_name"
                app:layout_constraintLeft_toRightOf="@+id/tv_phone_name"
                app:layout_constraintTop_toTopOf="@+id/tv_phone_name"/>
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/tv_exp_date_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="@+id/tv_phone_name"
                app:layout_constraintTop_toBottomOf="@+id/tv_phone_name"/>
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/tv_exp_date_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toRightOf="@+id/tv_exp_date_name"
                app:layout_constraintTop_toTopOf="@+id/tv_exp_date_name"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <TextView
            android:id="@+id/tv_change_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/BUTTON_CHANGE_PHONE_NUMBER"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/rel_phone"
            app:layout_constraintTop_toTopOf="@+id/rel_phone"/>
        <View
            android:id="@+id/change_phone_area"
            android:padding="@dimen/_20dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_change_phone"
            app:layout_constraintLeft_toLeftOf="@+id/tv_change_phone"
            app:layout_constraintRight_toRightOf="@+id/tv_change_phone"
            app:layout_constraintTop_toTopOf="@+id/tv_change_phone"/>
        <com.blink.academy.film.widgets.RoundButton
            android:id="@+id/btn_sign_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rel_phone"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_sign_out">
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/tv_service"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/BUTTON_TERMS_OF_USE"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/tv_policies"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/BUTTON_PRIVACY_POLICY"
                app:layout_constraintLeft_toRightOf="@+id/tv_service"
                app:layout_constraintTop_toTopOf="@+id/tv_service"/>
            <TextView
                android:textColor="@color/colorWhite"
                android:id="@+id/mail_support"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/CONTACT_SUPPORT"
                app:layout_constraintLeft_toRightOf="@+id/tv_policies"
                app:layout_constraintTop_toTopOf="@+id/tv_service"/>
            <TextView
                android:textColor="@color/color_deactivate"
                android:id="@+id/tv_write_off_account"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/LABEL_DEACTIVATE_ACCOUNT"
                app:layout_constraintLeft_toRightOf="@+id/mail_support"
                app:layout_constraintTop_toTopOf="@+id/tv_service"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/icp_cl"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true">
        <TextView
            android:textSize="8dp"
            android:textColor="@color/color_505050"
            android:id="@+id/tv_icp_filing"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/_2dp"
            android:text="@string/ICP_FILING"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/icp_arrow"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/icp_arrow"
            android:background="@drawable/icp_arrow_forward"
            android:layout_width="@dimen/_5dp"
            android:layout_height="@dimen/_5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>
