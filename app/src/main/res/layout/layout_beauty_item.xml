<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_beauty_item_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/root_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/iv_beauty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/icon_60_beauty_big_eyes"
            android:layout_centerHorizontal="true"/>
        <com.blink.academy.film.widgets.StrokeTextView
            android:gravity="center"
            android:id="@+id/tv_beauty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_beauty"
            android:layout_centerHorizontal="true"/>
        <RelativeLayout
            android:id="@+id/level_parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <com.blink.academy.film.widgets.StrokeCornerView
                android:id="@+id/tv_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:alpha="0"/>
        </RelativeLayout>
    </RelativeLayout>
</FrameLayout>
