            app:layout_constraintLeft_toRightOf="@+id/fps_click_area"
            app:layout_constraintTop_toTopOf="@+id/shutter_double_text_layout"/>
        <View
            android:id="@+id/iso_click_area"
            android:visibility="gone"
            android:layout_width="@dimen/_44dp"
            android:layout_height="@dimen/_44dp"
            app:layout_constraintBottom_toBottomOf="@+id/iso_double_text_layout"
            app:layout_constraintLeft_toRightOf="@+id/shutter_click_area"
            app:layout_constraintTop_toTopOf="@+id/iso_double_text_layout"/>
