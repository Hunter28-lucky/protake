            app:layout_constraintTop_toBottomOf="@+id/wave_parent"/>
        <com.blink.academy.film.widgets.camerainfo.DoubleTextLayout
            android:id="@+id/fps_double_text_layout"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/res_double_text_layout"
            app:layout_constraintTop_toTopOf="@+id/res_double_text_layout"/>
        <com.blink.academy.film.widgets.camerainfo.DoubleTextLayout
            android:id="@+id/shutter_double_text_layout"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/fps_double_text_layout"
            app:layout_constraintTop_toTopOf="@+id/res_double_text_layout"/>
        <com.blink.academy.film.widgets.camerainfo.DoubleTextLayout
