            app:center_vertical_text_view_bold="true"
            app:center_vertical_text_view_textColor="@color/colorWhite"
            app:center_vertical_text_view_textSize="@dimen/_10dp"
            app:layout_constraintLeft_toLeftOf="@+id/camera_area"
            app:layout_constraintRight_toRightOf="@+id/camera_area"
            app:layout_constraintTop_toBottomOf="@+id/home_audio_schedule"/>
        <View
            android:id="@+id/color_click_area"
            android:layout_width="@dimen/_44dp"
            android:layout_height="@dimen/_44dp"
            app:layout_constraintBottom_toBottomOf="@+id/color_btn"
            app:layout_constraintLeft_toLeftOf="@+id/color_btn"
            app:layout_constraintRight_toRightOf="@+id/color_btn"
            app:layout_constraintTop_toTopOf="@+id/color_btn"/>
        <View
            android:id="@+id/color_lock_area"
            android:layout_width="@dimen/_44dp"
            android:layout_height="@dimen/_44dp"
            app:layout_constraintBottom_toBottomOf="@+id/color_btn"
            app:layout_constraintLeft_toLeftOf="@+id/color_btn"
            app:layout_constraintRight_toRightOf="@+id/color_btn"
            app:layout_constraintTop_toTopOf="@+id/color_btn"/>
        <View
