            android:id="@+id/capture_exposure_bg"
            android:visibility="invisible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bgColor="#b2000000"
            app:cornerRadius="0dp"
            app:hollowtextSize="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/home_auto_switch_camera"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/home_auto_switch_camera"
            app:roundBottomLeft="true"
            app:roundBottomRight="true"
            app:roundTopLeft="true"
            app:roundTopRight="true"
            app:text="@string/RECORDING_ALERT_AE_AF_LOCK"/>
        <com.blink.academy.film.widgets.exposure.HollowTextView
            android:gravity="center"
            android:id="@+id/capture_exposure_lock_htv"
            android:visibility="invisible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bgColor="#ffba00"
            app:cornerRadius="0dp"
            app:hollowtextSize="10dp"
            app:layout_constraintBottom_toBottomOf="@+id/home_auto_switch_camera"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/home_auto_switch_camera"
