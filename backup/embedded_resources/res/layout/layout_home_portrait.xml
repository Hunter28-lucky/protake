            app:layout_constraintBottom_toBottomOf="@+id/play_btn"
            app:layout_constraintLeft_toLeftOf="@+id/play_btn"
            app:layout_constraintRight_toRightOf="@+id/play_btn"
            app:layout_constraintTop_toTopOf="@+id/play_btn"/>
        <View
            android:id="@+id/orientation_click_area"
            android:layout_width="@dimen/_44dp"
            android:layout_height="@dimen/_44dp"
            app:layout_constraintBottom_toBottomOf="@+id/screen_orientation_btn"
            app:layout_constraintLeft_toLeftOf="@+id/screen_orientation_btn"
            app:layout_constraintRight_toRightOf="@+id/screen_orientation_btn"
            app:layout_constraintTop_toTopOf="@+id/screen_orientation_btn"/>
