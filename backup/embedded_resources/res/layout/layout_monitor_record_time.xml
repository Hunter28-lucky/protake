<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_monitor_record_time_0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.blink.academy.film.widgets.RecordTimeFrameView
        android:id="@+id/record_time_frame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.blink.academy.film.widgets.StrokeTextView
        android:id="@+id/record_device_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="0"
        app:layout_constraintLeft_toLeftOf="@+id/record_time_frame"
        app:layout_constraintTop_toBottomOf="@+id/record_time_frame"/>
</androidx.constraintlayout.widget.ConstraintLayout>
