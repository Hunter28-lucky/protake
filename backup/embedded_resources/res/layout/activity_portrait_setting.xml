<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/activity_portrait_setting_0"
    android:background="#000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/root_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/root"
            android:background="#161616"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <RelativeLayout
                android:id="@+id/rl_top"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="#ffffff"
                    android:gravity="center"
                    android:id="@+id/tv_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:textAllCaps="true"/>
                <ImageView
                    android:id="@+id/close_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_30_close"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"/>
                <View
                    android:id="@+id/rl_add"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"/>
                <View
                    android:id="@+id/rl_scan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/rl_add"
                    android:layout_centerVertical="true"/>
                <ImageView
                    android:id="@+id/iv_scan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_30_preset_scan_qr"
                    android:scaleType="fitXY"
                    android:layout_toRightOf="@+id/iv_add"
                    android:layout_centerVertical="true"
                    android:contentDescription="@null"/>
                <ImageView
                    android:id="@+id/iv_add"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_30_preset_add_preset"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:contentDescription="@null"/>
                <View
                    android:id="@+id/close"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"/>
                <View
                    android:id="@+id/top_line"
                    android:background="@color/settingLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"/>
            </RelativeLayout>
            <FrameLayout
                android:id="@+id/fl_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.blink.academy.film.widgets.setting.PortraitRecordSettingView
                    android:id="@+id/record_view"
                    android:background="#00ffffff"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.PortraitDataSettingView
                    android:id="@+id/data_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.PortraitPresetSettingView
                    android:id="@+id/preset_view"
                    android:background="#00ffffff"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.PortraitAccessoriesSettingView
                    android:id="@+id/accessories_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.portrait.ProtakeSignInPortraitSettingView
                    android:id="@+id/protake_sign_in_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <com.blink.academy.film.widgets.setting.portrait.ProtakeProPortraitSettingView
                    android:id="@+id/protake_pro_view"
                    android:background="#00ffffff"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/ll_bottom"
                    android:background="#161616"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <RelativeLayout
                        android:id="@+id/rl_sign_in"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/iv_sign_in"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_48_tab_protake_plus"
                            android:layout_centerInParent="true"
                            android:contentDescription="@null"/>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_record"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/iv_record"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_48_tab_record"
                            android:layout_centerInParent="true"
                            android:contentDescription="@null"/>
                    </RelativeLayout>
                    <FrameLayout
                        android:id="@+id/fl_data"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:layout_gravity="center"
                            android:id="@+id/iv_data"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_48_tab_data"
                            android:contentDescription="@null"/>
                        <ImageView
                            android:layout_gravity="end"
                            android:id="@+id/iv_data_lock"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_20_lock"
                            android:contentDescription="@null"/>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/fl_preset"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:layout_gravity="center"
                            android:id="@+id/iv_preset"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_48_tab_presets"
                            android:contentDescription="@null"/>
                        <ImageView
                            android:layout_gravity="end"
                            android:id="@+id/iv_preset_lock"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_20_lock"
                            android:contentDescription="@null"/>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/fl_accessories"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:layout_gravity="center"
                            android:id="@+id/iv_accessories"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_48_tab_accessories"
                            android:contentDescription="@null"/>
                        <ImageView
                            android:layout_gravity="end"
                            android:id="@+id/iv_access_lock"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_20_lock"
                            android:contentDescription="@null"/>
                    </FrameLayout>
                    <RelativeLayout
                        android:id="@+id/rl_pro"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">
                        <ImageView
                            android:id="@+id/iv_pro"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/icon_48_tab_protake_plus"
                            android:layout_centerInParent="true"
                            android:contentDescription="@null"/>
                    </RelativeLayout>
                </LinearLayout>
                <View
                    android:background="@color/settingLine"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"/>
            </FrameLayout>
        </LinearLayout>
        <com.blink.academy.film.widgets.SubscribeDialog
            android:id="@+id/subscribe_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingCodeDialog
            android:id="@+id/code_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingGimbalDialog
            android:id="@+id/gimbal_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingProtakeFocusDialog
            android:id="@+id/protake_focus_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <com.blink.academy.film.widgets.dialog.SettingHintDialog
            android:id="@+id/hint_dialog"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <View
            android:id="@+id/click_view"
            android:background="@color/colorDiaBg"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <RelativeLayout
            android:id="@+id/privacy_dialog"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <com.blink.academy.film.widgets.dialog.login.PrivacyWebView
                android:id="@+id/privacy_web_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
