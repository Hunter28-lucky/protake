<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/root_parent"
    android:tag="layout/layout_setting_protake_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true">
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:id="@+id/lin_parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/lin_pro"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/iv_pro"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_50_plus_pro"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_pro"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textAllCaps="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/lin_looks"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/iv_looks"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_50_plus_color"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_looks"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textAllCaps="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/lin_data"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/iv_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_50_plus_data"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textAllCaps="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/lin_preset"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/iv_preset"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_50_plus_preset"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_preset"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textAllCaps="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:id="@+id/lin_access"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/iv_access"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_50_plus_accessories"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:id="@+id/tv_access"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textAllCaps="true"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:textColor="@color/colorWhite"
            android:id="@+id/tv_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lin_parent"/>
        <com.blink.academy.film.widgets.RoundButton
            android:id="@+id/btn_subscribe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_price"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/rel_sign_parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_subscribe">
            <LinearLayout
                android:id="@+id/rel_restore_purchases"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/iv_restore_purchases"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_20_restore_white"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/tv_restore_purchases"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/BUTTON_RESTORE_PURCHASE"
                    android:textAllCaps="true"/>
            </LinearLayout>
            <com.blink.academy.film.widgets.OneTakeProgressBar
                android:id="@+id/restore_progress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:alpha="0"
                android:indeterminateTint="@color/colorWhite"
                app:layout_constraintBottom_toBottomOf="@+id/rel_restore_purchases"
                app:layout_constraintLeft_toLeftOf="@+id/rel_restore_purchases"
                app:layout_constraintRight_toRightOf="@+id/rel_restore_purchases"
                app:layout_constraintTop_toTopOf="@+id/rel_restore_purchases"/>
            <LinearLayout
                android:id="@+id/rel_sign"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toRightOf="@+id/rel_restore_purchases"
                app:layout_constraintTop_toTopOf="@+id/rel_restore_purchases">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/iv_sign"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_20_sign_in_white"/>
                <TextView
                    android:textColor="@color/colorWhite"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/tv_sign_in"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAllCaps="true"/>
            </LinearLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/rel_write_off"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/rel_sign"
                app:layout_constraintLeft_toRightOf="@+id/rel_sign"
                app:layout_constraintTop_toTopOf="@+id/rel_sign">
                <ImageView
                    android:layout_gravity="center_vertical"
                    android:id="@+id/iv_write_off_account"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/icon_30_close"
                    app:layout_constraintBottom_toBottomOf="@+id/tv_write_off_account"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/tv_write_off_account"
                    app:tint="@color/color_deactivate"/>
                <TextView
                    android:textColor="@color/color_deactivate"
                    android:id="@+id/tv_write_off_account"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAllCaps="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@+id/iv_write_off_account"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:id="@+id/v_line"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rel_sign_parent"/>
        <TextView
            android:textColor="@color/colorWhite"
            android:id="@+id/tv_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/v_line"/>
        <TextView
            android:textColor="@color/colorWhite"
            android:id="@+id/tv_policies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/tv_service"
            app:layout_constraintTop_toTopOf="@+id/tv_service"/>
        <TextView
            android:textColor="@color/colorWhite"
            android:id="@+id/tv_support_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/tv_policies"
            app:layout_constraintTop_toTopOf="@+id/tv_policies"/>
        <TextView
            android:textColor="@color/colorWhite"
            android:id="@+id/tv_info"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_service"/>
        <View
            android:id="@+id/v_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_service"
            app:layout_constraintLeft_toLeftOf="@+id/tv_service"
            app:layout_constraintRight_toRightOf="@+id/tv_service"
            app:layout_constraintTop_toTopOf="@+id/tv_service"/>
        <View
            android:id="@+id/v_policies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_policies"
            app:layout_constraintLeft_toLeftOf="@+id/tv_policies"
            app:layout_constraintRight_toRightOf="@+id/tv_policies"
            app:layout_constraintTop_toTopOf="@+id/tv_policies"/>
        <View
            android:id="@+id/v_support_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/tv_support_email"
            app:layout_constraintLeft_toLeftOf="@+id/tv_support_email"
            app:layout_constraintRight_toRightOf="@+id/tv_support_email"
            app:layout_constraintTop_toTopOf="@+id/tv_support_email"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/icp_cl"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true">
        <TextView
            android:textSize="8dp"
            android:textColor="@color/color_505050"
            android:id="@+id/tv_icp_filing"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/_2dp"
            android:text="@string/ICP_FILING"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/icp_arrow"
            app:layout_constraintTop_toTopOf="parent"/>
        <ImageView
            android:id="@+id/icp_arrow"
            android:background="@drawable/icp_arrow_forward"
            android:layout_width="@dimen/_5dp"
            android:layout_height="@dimen/_5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>
