<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_receiver_view_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.blink.academy.film.widgets.StrokeTextView
        android:id="@+id/has_connected_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MONITOR_SETTINGS_SECTOR_CONNECTED_MONITORS"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/receiver_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="@+id/has_connected_title"
        app:layout_constraintTop_toBottomOf="@+id/has_connected_title"/>
</androidx.constraintlayout.widget.ConstraintLayout>
