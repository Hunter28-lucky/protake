<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_transmit_setting_page1_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.blink.academy.film.widgets.StrokeTextView
        android:id="@+id/equipment_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MONITOR_SETTINGS_SECTOR_DEVICE_ROLE"
        android:textAllCaps="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/equipment_camera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintLeft_toLeftOf="@+id/equipment_title"
        app:layout_constraintTop_toBottomOf="@+id/equipment_title"/>
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/equipment_monitor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintLeft_toRightOf="@+id/equipment_camera"
        app:layout_constraintTop_toTopOf="@+id/equipment_camera"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/quality_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/equipment_title">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/quality_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MONITOR_SETTINGS_SECTOR_PRIORITY"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/quality_high"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="@+id/quality_title"
            app:layout_constraintTop_toBottomOf="@+id/quality_title"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/quality_quick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/quality_high"
            app:layout_constraintTop_toTopOf="@+id/quality_high"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/quality_latency"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/quality_quick"
            app:layout_constraintTop_toTopOf="@+id/quality_quick"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/job_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/equipment_title">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/job_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MONITOR_SETTINGS_SECTOR_JOB"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/job_director"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="@+id/job_title"
            app:layout_constraintTop_toBottomOf="@+id/job_title"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/job_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/job_director"
            app:layout_constraintTop_toTopOf="@+id/job_director"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/job_customer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/job_camera"
            app:layout_constraintTop_toTopOf="@+id/job_director"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/job_other"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/job_customer"
            app:layout_constraintTop_toTopOf="@+id/job_director"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/camera_unit_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/equipment_camera">
        <com.blink.academy.film.widgets.StrokeTextView
            android:gravity="center"
            android:id="@+id/camera_unit_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEVICE_CODE_LOCAL"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/camera_unit_a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/camera_unit_title"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/camera_unit_b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/camera_unit_a"
            app:layout_constraintTop_toTopOf="@+id/camera_unit_a"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/camera_unit_c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/camera_unit_b"
            app:layout_constraintTop_toTopOf="@+id/camera_unit_a"/>
        <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
            android:id="@+id/camera_unit_d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toRightOf="@+id/camera_unit_c"
            app:layout_constraintTop_toTopOf="@+id/camera_unit_a"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/camera_device_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/equipment_title"
        app:layout_constraintTop_toBottomOf="@+id/equipment_camera">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/camera_device_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/SETTINGS_SUFFIX"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/camera_device_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="@+id/camera_device_title"
            app:layout_constraintTop_toBottomOf="@+id/camera_device_title"/>
        <FrameLayout
            android:id="@+id/fl_reset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/camera_device_number"
            app:layout_constraintLeft_toRightOf="@+id/camera_device_number"
            app:layout_constraintTop_toTopOf="@+id/camera_device_number">
            <com.blink.academy.film.widgets.StrokeRoundView
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <TextView
                android:textColor="#000000"
                android:layout_gravity="center"
                android:id="@+id/tv_reset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="reset"
                android:textAllCaps="true"/>
        </FrameLayout>
        <View
            android:id="@+id/reset_click"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/fl_reset"
            app:layout_constraintLeft_toLeftOf="@+id/fl_reset"
            app:layout_constraintRight_toRightOf="@+id/fl_reset"
            app:layout_constraintTop_toTopOf="@+id/fl_reset"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/camera_control_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/camera_unit_parent">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/camera_control_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MONITOR_SETTINGS_SECTOR_ALLOW_CONTROL"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.SwitchButton
            android:id="@+id/sb_control_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/camera_control_title"
            app:sb_text_check="@string/TOGGLE_ON"
            app:sb_text_uncheck="@string/TOGGLE_OFF"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/monitor_control_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/camera_device_parent">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/monitor_control_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MONITOR_SETTINGS_SECTOR_CONTROL"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.SwitchButton
            android:id="@+id/sb_control_monitor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/monitor_control_title"
            app:sb_text_check="@string/TOGGLE_ON"
            app:sb_text_uncheck="@string/TOGGLE_OFF"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cloud_camera_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/camera_control_parent">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/cloud_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DEVICE_CODE_CLOUD"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.StrokeTextView
            android:gravity="left"
            android:id="@+id/cloud_number_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="44dp"
            android:textAllCaps="true"
            app:layout_constraintBottom_toBottomOf="@+id/sb_cloud_camera"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/sb_cloud_camera"/>
        <com.blink.academy.film.widgets.OneTakeProgressBar
            android:id="@+id/pb_cloud"
            android:visibility="visible"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:tint="#ffffff"
            android:indeterminateTint="#ffffff"
            app:layout_constraintBottom_toBottomOf="@+id/cloud_number_tv"
            app:layout_constraintLeft_toLeftOf="@+id/cloud_number_tv"
            app:layout_constraintRight_toRightOf="@+id/cloud_number_tv"
            app:layout_constraintTop_toTopOf="@+id/cloud_number_tv"/>
        <FrameLayout
            android:id="@+id/fl_reset_room_id"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/sb_cloud_camera"
            app:layout_constraintLeft_toRightOf="@+id/cloud_number_tv"
            app:layout_constraintTop_toTopOf="@+id/sb_cloud_camera">
            <com.blink.academy.film.widgets.StrokeRoundView
                android:id="@+id/srv_reset"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <TextView
                android:textColor="#000000"
                android:layout_gravity="center"
                android:id="@+id/tv_reset_room_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="reset"
                android:textAllCaps="true"/>
        </FrameLayout>
        <com.blink.academy.film.widgets.SwitchButton2
            android:id="@+id/sb_cloud_camera"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/fl_reset_room_id"
            app:layout_constraintTop_toBottomOf="@+id/cloud_title_tv"
            app:sb_text_check="@string/TOGGLE_ON"
            app:sb_text_uncheck="@string/TOGGLE_OFF"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/monitor_time_parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/monitor_control_parent">
        <com.blink.academy.film.widgets.StrokeTextView
            android:id="@+id/extend_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/MONITOR_TIME_LEFT_MONITOR"
            android:textAllCaps="true"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.blink.academy.film.widgets.StrokeTextView
            android:gravity="left"
            android:id="@+id/time_left_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:minWidth="44dp"
            android:textAllCaps="true"
            app:layout_constraintBottom_toBottomOf="@+id/fl_extend"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/fl_extend"/>
        <FrameLayout
            android:id="@+id/fl_extend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintLeft_toRightOf="@+id/time_left_tv"
            app:layout_constraintTop_toBottomOf="@+id/extend_title_tv">
            <com.blink.academy.film.widgets.StrokeRoundView
                android:id="@+id/srv_extend"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <TextView
                android:textColor="#000000"
                android:layout_gravity="center"
                android:id="@+id/tv_extend"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="extend"
                android:textAllCaps="true"/>
        </FrameLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
