<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_monitor_player_control_0"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/iv_pre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_34_player_rewind"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/iv_play"
        app:layout_constraintRight_toLeftOf="@+id/iv_rewind"
        app:layout_constraintTop_toTopOf="@+id/iv_play"/>
    <ImageView
        android:id="@+id/iv_rewind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_34_player_rewind_5sec"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/iv_play"
        app:layout_constraintRight_toLeftOf="@+id/iv_play"
        app:layout_constraintTop_toTopOf="@+id/iv_play"/>
    <ImageView
        android:id="@+id/iv_play"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_34_player_play"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/iv_forward"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_34_player_forward_5sec"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/iv_play"
        app:layout_constraintLeft_toRightOf="@+id/iv_play"
        app:layout_constraintTop_toTopOf="@+id/iv_play"/>
    <ImageView
        android:id="@+id/iv_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_34_player_next"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/iv_play"
        app:layout_constraintLeft_toRightOf="@+id/iv_forward"
        app:layout_constraintTop_toTopOf="@+id/iv_play"/>
    <View
        android:id="@+id/iv_play_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_play"
        app:layout_constraintLeft_toLeftOf="@+id/iv_play"
        app:layout_constraintRight_toRightOf="@+id/iv_play"
        app:layout_constraintTop_toTopOf="@+id/iv_play"/>
    <View
        android:id="@+id/iv_pre_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_pre"
        app:layout_constraintLeft_toLeftOf="@+id/iv_pre"
        app:layout_constraintRight_toRightOf="@+id/iv_pre"
        app:layout_constraintTop_toTopOf="@+id/iv_pre"/>
    <View
        android:id="@+id/iv_rewind_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_rewind"
        app:layout_constraintLeft_toLeftOf="@+id/iv_rewind"
        app:layout_constraintRight_toRightOf="@+id/iv_rewind"
        app:layout_constraintTop_toTopOf="@+id/iv_rewind"/>
    <View
        android:id="@+id/iv_forward_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_forward"
        app:layout_constraintLeft_toLeftOf="@+id/iv_forward"
        app:layout_constraintRight_toRightOf="@+id/iv_forward"
        app:layout_constraintTop_toTopOf="@+id/iv_forward"/>
    <View
        android:id="@+id/iv_next_click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/iv_next"
        app:layout_constraintLeft_toLeftOf="@+id/iv_next"
        app:layout_constraintRight_toRightOf="@+id/iv_next"
        app:layout_constraintTop_toTopOf="@+id/iv_next"/>
</androidx.constraintlayout.widget.ConstraintLayout>
