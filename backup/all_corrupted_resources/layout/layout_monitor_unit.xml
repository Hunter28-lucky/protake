<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:tag="layout/layout_monitor_unit_0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/monitor_unit_a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/monitor_unit_b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="@+id/monitor_unit_a"
        app:layout_constraintLeft_toRightOf="@+id/monitor_unit_a"/>
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/monitor_unit_c"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="@+id/monitor_unit_a"
        app:layout_constraintLeft_toRightOf="@+id/monitor_unit_b"/>
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/monitor_unit_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="@+id/monitor_unit_a"
        app:layout_constraintLeft_toRightOf="@+id/monitor_unit_c"/>
    <com.blink.academy.film.widgets.StrokeTextViewWithBottomBlock
        android:id="@+id/monitor_unit_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="true"
        app:layout_constraintBottom_toBottomOf="@+id/monitor_unit_a"
        app:layout_constraintLeft_toRightOf="@+id/monitor_unit_d"/>
</androidx.constraintlayout.widget.ConstraintLayout>
