<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="22dp"
                android:width="22dp"/>
            <solid android:color="#22000000"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape android:shape="oval">
            <size
                android:height="20dp"
                android:width="20dp"/>
            <solid android:color="#ffffff"/>
            <stroke
                android:width="1dp"
                android:color="#ffffff"/>
        </shape>
    </item>
</layer-list>
